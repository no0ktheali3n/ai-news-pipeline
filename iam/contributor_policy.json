{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Action": [
        "lambda:InvokeFunction",
        "lambda:ListFunctions",
        "bedrock:InvokeModel",
        "bedrock:ListFoundationModels",
        "secretsmanager:GetSecretValue",
        "logs:GetLogEvents",
        "logs:DescribeLogStreams",
        "logs:FilterLogEvents",
        "s3:GetObject",
        "s3:ListBucket"
      ],
      "Resource": "*"
    }
  ]
}